.content{position:fixed;top:0;left:0;display:flex;flex-direction:column;align-items:center;justify-content:center}.wrapperBox{position:relative;width:100vw;height:100vh;z-index:999;overflow:hidden}#wrapper{width:100%;height:100%;top:0;left:0}.search-container{position:absolute;top:90px;left:10px;z-index:1000;background:rgba(255,255,255,.9);padding:10px;border-radius:8px;box-shadow:0 2px 6px rgba(0,0,0,.1);display:flex;gap:8px}.search-container button{width:60px!important}.info-box{position:absolute;bottom:20px;left:20px;z-index:1000;background:rgba(255,255,255,.9);padding:10px;border-radius:8px;box-shadow:0 2px 6px rgba(0,0,0,.1);max-width:300px}.search-results{position:absolute;top:150px;left:10px;z-index:1000;background:#fff;width:300px;border-radius:8px;box-shadow:0 2px 6px rgba(0,0,0,.1)}.result-item{padding:10px;border-bottom:1px solid #eee;cursor:pointer}.result-item:last-child{border-bottom:none}.result-item:hover{background-color:#f5f5f5}
